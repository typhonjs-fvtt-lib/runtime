    },
    "./svelte/handler": {
      "import": "./dist/svelte/handler.js"
    },
    "./svelte/helper": {
      "import": "./dist/svelte/helper.js"
    },
    "./svelte/legacy": {
      "import": "./dist/svelte/application-legacy.js"
