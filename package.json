    "rollup-plugin-sourcemaps": "^0.6",
    "rollup-plugin-svelte": "^7",
    "rollup-plugin-terser": "^7",
    "sass": "^1",
    "svelte-preprocess": "^4"
  },
  "scripts": {
    "build-dist": "rollup --config",
    "build-dist-watch": "rollup --watch --config",
    "eslint": "eslint .",
    "esdoc": "esdoc"
